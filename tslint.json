{
    "extends": [
        "tslint-react"
    ],
    "defaultSeverity": "warn",    
    "rules": {
        "class-name": true,
        "jsx-no-multiline-js": false,
        "jsx-no-lambda": false,
        "comment-format": [
            true,
            "check-space"
        ],
        "curly": true,
        "forin": true,
        "indent": [
            true,
            "spaces"
        ],
        "no-duplicate-super": true,
        "label-position": true,
        "no-arg": true,
        "no-bitwise": true,
        "no-console": [
            true,
            "log",
            "debug",
            "info",
            "time",
            "timeEnd",
            "trace"
        ],
        "no-construct": true,
        "no-debugger": true,
        "no-duplicate-variable": true,
        "no-empty": false,
        "no-eval": true,
        "no-shadowed-variable": false,
        "no-string-literal": false,
        "no-switch-case-fall-through": false,
        "trailing-comma": [
            false
        ],
        "no-trailing-whitespace": false,
        "no-unused-expression": true,
        "no-require-imports": true,
        "ordered-imports": false,
        "no-use-before-declare": false,
        "no-var-keyword": true,
        "one-line": [
            true,
            "check-open-brace",
            "check-catch",
            "check-else",
            "check-whitespace"
        ],
        "radix": true,
        "semicolon": [
            true            
        ],
        "triple-equals": [
            true,
            "allow-null-check"
        ],
        "whitespace": [
            true,
            "check-branch",
            "check-decl",
            "check-operator",
            "check-separator",
            "check-type"
        ],
        "quotemark": [
            true,
            "double",
            "jsx-double"
        ],
        "avoidEscape": true,
        "jsx-boolean-value": false
    }
}